<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entities.UserItemEntity">
        <table name="UserItem" schema="DVDCollection" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="username">
                <column name="username" length="30"/>
            </basic>
            <basic name="password">
                <column name="password" length="30"/>
            </basic>
            <basic name="type">
                <column name="type" precision="11"/>
            </basic>
            <basic name="state">
                <column name="state" precision="1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
